{
    "compilerOptions": {
      "module": "esnext",
      "moduleResolution": "node",
      "declaration": true,
      "noImplicitAny": false,
      "removeComments": true,
      "noLib": false,
      "noEmit": true, 
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "target": "esnext",
      "sourceMap": true,    
      "jsx": "react",
      "skipLibCheck": true,
      // "strict": true,
      "lib": [
        "es6"
      ]
    },
    "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "AppData"
    ]
   }